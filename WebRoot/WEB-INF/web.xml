<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4" 
	xmlns="http://java.sun.com/xml/ns/j2ee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee 
	http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>ShowReport</servlet-name>
    <servlet-class>servlet.ShowReport</servlet-class>
    <load-on-startup>10</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>ShowReport</servlet-name>
    <url-pattern>/ShowReport</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>QueryReport</servlet-name>
    <servlet-class>servlet.QueryReport</servlet-class>
    <load-on-startup>10</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>QueryReport</servlet-name>
    <url-pattern>/QueryReport</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SubmitTasks</servlet-name>
    <servlet-class>servlet.SubmitTasks</servlet-class>
    <load-on-startup>10</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>SubmitTasks</servlet-name>
    <url-pattern>/SubmitTasks</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>MulitTasks</servlet-name>
    <servlet-class>servlet.MulitTasks</servlet-class>
    <load-on-startup>10</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>MulitTasks</servlet-name>
    <url-pattern>/MulitTasks</url-pattern>
  </servlet-mapping>
  <listener>
    <listener-class>servlet.ContractTimingListener</listener-class>
  </listener>
  <error-page>
    <error-code>404</error-code>
    <location>/errot.html</location>
  </error-page>
</web-app>
